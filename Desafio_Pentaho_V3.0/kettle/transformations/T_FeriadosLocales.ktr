        <jsScript_script>var provincia = Provincia;
var localidad = Localidad;
var cantidad = Cantidad;

var longitudDeseada = 60;


if (provincia.length &lt; longitudDeseada) {
    var espacios = "";
    for (var i = provincia.length; i &lt; longitudDeseada; i++) {
        espacios += " ";
    }
    provincia = provincia + espacios;
} else if (provincia.length > longitudDeseada) {
    provincia = provincia.substring(0, longitudDeseada);
}

if (localidad.length &lt; longitudDeseada) {
    var espacios = "";
    for (var i = localidad.length; i &lt; longitudDeseada; i++) {
        espacios += " ";
    }
    localidad = localidad + espacios;
} else if (localidad.length > longitudDeseada) {
    localidad = localidad.substring(0, longitudDeseada);
}


longitudDeseada = 5; // Longitud deseada para el número de cantidad

// Verificar y ajustar la longitud si es necesario
if (cantidad.length &lt; longitudDeseada) {
    var ceros = "";
    for (var i = 0; i &lt; longitudDeseada - cantidad.length; i++) {
        ceros += "0";
    }
    cantidad = ceros + cantidad;
} else if (cantidad.length > longitudDeseada) {
    cantidad = cantidad.substring(0, longitudDeseada); 
}
</jsScript_script>
