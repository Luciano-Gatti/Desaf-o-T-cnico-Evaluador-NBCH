        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>//Script here
var cod_postal = Cod_Postal;
var resultado = Resultado;
var pi = PI;
var fecha = Fecha;
var provincia = Provincia;
var localidad = Localidad;

// Función para verificar si un valor puede ser convertido a un entero
function isNumeric(value) {
  return !isNaN(value) &amp;&amp; isFinite(value);
}

// Función para verificar si un valor puede ser convertido a un fecha con formato dd/MM/yyyy
function isValidDate(dateString, format) {
  var regex = /^(\d{2})\/(\d{2})\/(\d{4})$/;
  var match = dateString.match(regex);
  if (match === null) {
    return false;
  }
  var day = parseInt(match[1], 10);
  var month = parseInt(match[2], 10);
  var year = parseInt(match[3], 10);

  if (month &lt; 1 || month > 12) {
    return false;
  }
  var daysInMonth = new Date(year, month, 0).getDate();
  return day > 0 &amp;&amp; day &lt;= daysInMonth;
}

// Verificar y parsear Cod_Postal
if (!isNumeric(cod_postal)) {
	resultado = resultado ? resultado + ", Formato de cód. postal incorrecto" : "Formato de cód. postal incorrecto";
}else{
	cod_postal = parseInt(cod_postal, 10);
	if(cod_postal > 999999999999999){
		resultado = resultado ? resultado + ", El cód. postal supera los 15 dígitos" : "El cód. postal supera los 15 dígitos";
	}
}

// Verificar y parsear PI
if (!isNumeric(pi)) {
	resultado = resultado ? resultado + ", Formato de P.I. incorrecto" : "Formato de P.I. incorrecto";
}else{
	pi = parseInt(pi, 10);
	if(pi > 99999){
		resultado = resultado ? resultado + ", El P.I. supera los 5 dígitos" : "El P.I. supera los 5 dígitos" ;
	}
}

// Verificar formato de fecha
if (!isValidDate(fecha, "dd/MM/yyyy")) {
	resultado = resultado ? resultado + ", Formato de fecha incorrecto" : "Formato de fecha incorrecto";
}

//Verificar longitud de la Provincia
if (provincia.length > 60) {
	resultado = resultado ? resultado +  ", La Provincia supera los 60 dígitos" : "La Provincia supera los 60 dígitos";
}

//Verificar longitud de la Localidad
if (localidad.length > 60) {
	resultado = resultado ? resultado + ", La localidad supera los 60 dígitos" : "La localidad supera los 60 dígitos";
}

</jsScript_script>
